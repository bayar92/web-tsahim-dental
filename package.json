{
  "name": "tsahim-dental-public",
  "version": "0.1.1",
  "private": true,
  "type": "module",
  "prisma": {
    "seed": "ts-node -T prisma/seed.ts"
  },
  "scripts": {
    "postinstall": "husky install",
    "dev": "next dev",
    "build": "prisma generate && next build",
    "start": "cross-env NODE_ENV=production node server.js",
    "lint": "next lint",
    "gen": "fx add resource",
    "prisma:migrate:dev": "TS_NODE_COMPILER_OPTIONS='{\"module\":\"commonjs\"}' dotenv -e .env.local -- prisma migrate dev",
    "prisma:migrate:prod": "TS_NODE_COMPILER_OPTIONS='{\"module\":\"commonjs\"}' dotenv -e .env.local -- prisma migrate deploy",
    "prisma:generate": "dotenv -e .env.local -- prisma generate",
    "prisma:seed:dev": "TS_NODE_COMPILER_OPTIONS='{\"module\":\"commonjs\"}' dotenv -e .env.local -- prisma db seed",
    "prisma:seed:staging": "TS_NODE_COMPILER_OPTIONS='{\"module\":\"commonjs\"}' dotenv -e .env.staging.local -- prisma db seed",
    "prisma:seed:prod": "TS_NODE_COMPILER_OPTIONS='{\"module\":\"commonjs\"}' dotenv -e .env.production.local -- prisma db seed",
    "prisma:reset": "TS_NODE_COMPILER_OPTIONS='{\"module\":\"commonjs\"}' dotenv -e .env.local -- prisma migrate reset",
    "prisma:studio": "prisma studio",
    "stripe:dev": "stripe listen --forward-to  http://localhost:3000/api/stripe/webhook",
    "docker:start": "docker-compose up -d",
    "docker:stop": "docker-compose down",
    "heroku-postbuild": "next build && npm run prisma:migrate:prod"
  },
  "dependencies": {
